server {
    listen 80;
    server_name 185.239.209.58;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /root/wenzo/latestmond/static/;
        #alias /root/wenzo/usaJobsFinal/static/;
    }


     # Route requests for the 'latestmond' application
    location /{
        include proxy_params;
        proxy_pass http://unix:/run/latestmond-gunicorn.sock;
    }

     # Route requests for the 'usaJobsFinal' application
   location /frs/GWWNCapps/US/usajobs/{
        include proxy_params;
        proxy_pass http://unix:/run/usajobs-gunicorn.sock;        
    }
    location /frs/GWWNCapps/kenya/kenyajobs/{
        include proxy_params;
        proxy_pass http://unix:/run/latestmond-gunicorn.sock;        
    }
    # Route requests for the 'GlassDoor' application
   location /frs/{
        include proxy_params;
        proxy_pass http://unix:/run/scrapping-glassdoor-gunicorn.sock;     
    }
     # Route requests for the 'GlassDoor' application
  location /frss/{
        include proxy_params;
        proxy_pass http://unix:/run/scrapping-glassdoor-gunicorn.sock;     
    }
     # Route requests for the 'GlassDoor' application
   location /frsscrapping/admin/{
        include proxy_params;
        proxy_pass http://unix:/run/scrapping-glassdoor-gunicorn.sock;        
    }
   location /frs-jenkins/{
        include proxy_params;
        proxy_pass http://unix:/run/jenkins-deployment-gunicorn.sock;        
    }
   location /pgadmin4/{
           proxy_pass http://pgadmin/;
            #proxy_pass http://localhost:5050/
            proxy_redirect http:// $scheme://;
            proxy_set_header Host $host;
            proxy_set_header X-Real-IP $remote_addr;
            proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
            proxy_set_header X-Script-Name  /pgadmin;
}
}
